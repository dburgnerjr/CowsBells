<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.danielburgnerjr.cowsbells"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="23" />

    <application
        android:allowBackup="true"
        android:debuggable="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:testOnly="true"
        android:theme="@style/CowsBellsOpenScreenTheme" >
        <activity
            android:name="com.danielburgnerjr.cowsbells.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.danielburgnerjr.cowsbells.GameActivity"
            android:label="New Game"
            android:theme="@style/AppBaseTheme" >
        </activity>
    </application>

</manifest>